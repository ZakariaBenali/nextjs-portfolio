{
  "name": "nextjs-portfolio",
  "version": "0.1.0",
  "private": true,
  "description": "A custom-built portfolio created with Next.js to showcase your skills and work, to connect with a wider range of potential clients.",
  "author": "Zakaria Benali <benalizakaria.me@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "types:check": "tsc --noEmit",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@next/font": "^13.1.1",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.3.2",
    "clsx": "^1.2.1",
    "formik": "^2.2.9",
    "framer-motion": "^9.0.2",
    "next": "13.1.1",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/client-preset": "2.0.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@next/env": "^13.1.6",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-dom": "18.0.8",
    "@types/react-google-recaptcha": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "graphql": "^16.6.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "4.8.4"
  }
}